<%= f.label :tag_list, '国際' %>
<%= text_field_tag 'topic[tag_list]', @topic.tag_list, :class => "form-control" %>






<%= search_form_for(@topic) do |f| %>
  
  <div class="form-group">
    <%= f.label :tag_list %>
    <%= f.text_field :tag_list, value: @topic.tag_list.join(','), class: "form-control" %>
  </div>
  <div class="pull-right">
    <%= f.submit '登録する', class: "btn btn-info" %>
  </div>
<% end %>